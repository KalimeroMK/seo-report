{
    "name": "kalimeromk/seo-report",
    "description": "Framework-agnostic PHP SEO analysis: analyze URL/sitemap, return API result (JSON). Works with Laravel, Yii, or plain PHP.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0|^12.0",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "KalimeroMK\\SeoReport\\": "src/"
        },
        "files": [
          "src/Support/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "KalimeroMK\\SeoReport\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "analyse": "php -d memory_limit=512M vendor/bin/phpstan analyse",
        "rector": "vendor/bin/rector process",
        "rector-dry": "vendor/bin/rector process --dry-run"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
